FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    arch        "LSB;label=32;scalar=64";
    object      DtTr_08;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

inlet
{
    type            calculated;
    value           uniform 0.001;
}

left
{
    type            symmetry;
}

outlet
{
    type            calculated;
    value           uniform 0.001;
}

right
{
    type            symmetry;
}

top
{
    type            symmetry;
}

ground_inner
{
    type            DtWallFunction;
    value           uniform 0.001;
}

ground_outer
{
    type            DtWallFunction;
    value           uniform 0.001;
}

bridges
{
    type            DtWallFunction;
    value           uniform 0.001;
}

buildings
{
    type            DtWallFunction;
    value           uniform 0.001;
}


