FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    arch        "LSB;label=32;scalar=64";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

left
{
    type            symmetryPlane;
}

right
{
    type            symmetryPlane;
}

inlet
{
    type            calculated;
    value           uniform 0.001;
}

top
{
    type            symmetryPlane;
}

offsite_bld
{
    type            nutUSpaldingWallFunction;
    value           uniform 0.001;
}

onsite_bld
{
    type            nutUSpaldingWallFunction;
    value           uniform 0.001;
}

terrain
{
    type            nutkAtmRoughWallFunction;
    z0              0.03;
    value           uniform 0.001;
}

terrain_bld
{
    type            nutkAtmRoughWallFunction;
    z0              0.03;
    value           uniform 0.001;
}

outlet
{
    type            zeroGradient;
}


